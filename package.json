{
  "name": "@tenry/graphql-decorators",
  "version": "0.1.0",
  "description": "Generate GraphQL schema with annotated classes.",
  "keywords": [
    "graphql",
    "decorators"
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "test": "mocha dist/test",
    "test:coverage": "nyc npm run test"
  },
  "author": "Simon \"Tenry\" Burchert",
  "license": "MIT",
  "dependencies": {
    "graphql": "^0.13.1",
