    "reflect-metadata": "^0.1.12"
  },
  "devDependencies": {
    "@types/graphql": "^0.12.5",
    "@types/mocha": "^2.2.48",
    "chai": "^4.1.2",
    "mocha": "^5.0.4",
    "nyc": "^11.6.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  },
  "files": [
    "dist/index.js",
    "dist/decorators.js",
    "dist/utils.js"
  ],
  "nyc": {
    "include": [
      "dist"
